{
    "$schema": "https://raw.githubusercontent.com/LittleLittleCloud/Agent-ChatRoom/main/schema/chatroom_web_search_configuration_schema.json",
    "bing_search_config": {
        "name": "bing-search",
        "bing_api_key": "<your-bing-api-key>",
        "openai_config": {
            "llm_type": "OpenAI",
            "openai_model_id": "gpt-3.5-turbo",
            "openai_api_key": "<your-api-key>"
        }
    },
    "google_search_config": {
        "name": "google-search",
        "google_api_key": "<your-google-api-key>",
        "google_custom_search_engine_id": "<your-google-custom-search-engine-id>",
        "openai_config": {
            "llm_type": "OpenAI",
            "openai_model_id": "gpt-3.5-turbo",
            "openai_api_key": "<your-api-key>"
        }
    }
}
