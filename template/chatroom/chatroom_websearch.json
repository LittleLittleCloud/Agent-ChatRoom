            }
        },
        "google_search_config": {
            "google_api_key": "<your-api-key>",
            "google_custom_search_engine_id": "<your-cse-id>",
            "openai_config": {
                "llm_type": "OpenAI",
                "openai_api_key": "<your-api-key>",
                "openai_model_id": "gpt-3.5-turbo"
            }
        }
